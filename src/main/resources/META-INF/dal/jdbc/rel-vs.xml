<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.zeus.dal.core" gen="true" do-class-suffix="Do">
    <entity name="r-vs-slb" table="r_vs_slb" alias="rvs" do-class="RelVsSlbDo">
        <query-defs>
            <query name="find-all-vses-by-slb" type="SELECT" multiple="true">
                <param name="slb-id"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='slb-id'/> = ${slb-id}
                    ]]>
                </statement>
            </query>
            <query name="find-slb-by-vs" type="SELECT">
                <param name="vs-id"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='vs-id'/> = ${vs-id}
                    ]]>
                </statement>
            </query>
            <query name="insert-all" type="INSERT" batch="true">
                <statement>
                    <![CDATA[
                    INSERT INTO <TABLE/>(<FIELDS/>)
                    VALUES(<VALUES/>)
                    ]]>
                </statement>
            </query>
            <query name="delete-all-by-slb" type="DELETE">
                <param name="slb-id"/>
                <statement>
                    <![CDATA[
                    DELETE FROM <TABLE/>
                    WHERE <FIELD name='slb-id'/> = ${slb-id}
                    ]]>
                </statement>
            </query>
            <query name="delete-by-vs" type="DELETE" batch="true">
                <param name="vs-id"/>
                <statement>
                    <![CDATA[
                    DELETE FROM <TABLE/>
                    WHERE <FIELD name='vs-id'/> = ${vs-id}
                    ]]>
                </statement>
            </query>
        </query-defs>
    </entity>
    <entity name="r-vs-domain" table="r_vs_domain" alias="rvd" do-class="RelVsDomainDo">
        <var name="ids" value-type="Long[]"/>
        <query-defs>
            <query name="find-all-domains-by-vses" type="SELECT" multiple="true">
                <param name="ids"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='vs-id'/> in <IN>${ids}</IN>
                    ]]>
                </statement>
            </query>
            <query name="find-all-vses-by-domain" type="SELECT" multiple="true">
                <param name="domain"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='domain'/> = ${domain}
                    ]]>
                </statement>
            </query>
            <query name="delete-by-vs-and-domain" type="DELETE">
                <param name="vs-id"/>
                <param name="domain"/>
                <statement>
                    <![CDATA[
                    DELETE FROM <TABLE/>
                    WHERE <FIELD name='domain'/> = ${domain}
                        AND <FIELD name='vs-id'/> = ${vs-id}
                    ]]>
                </statement>
            </query>
            <query name="delete-all-by-vs" type="DELETE" batch="true">
                <param name="vs-id"/>
                <statement>
                    <![CDATA[
                    DELETE FROM <TABLE/>
                    WHERE <FIELD name='vs-id'/> = ${vs-id}
                    ]]>
                </statement>
            </query>
        </query-defs>
    </entity>
</entities>

